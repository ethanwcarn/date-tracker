files:
  "/opt/elasticbeanstalk/hooks/appdeploy/post/99_kill_default_process.sh":
    mode: "000755"
    owner: root
    group: root
    content: |
      #!/bin/bash
      # Get the process ID of the default Python process
      PID=$(pgrep -f "python.*app.py")
      if [ ! -z "$PID" ]; then
        kill $PID
      fi


